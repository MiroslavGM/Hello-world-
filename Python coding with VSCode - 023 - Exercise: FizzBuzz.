def fizz_buzz(input):
  if input % 3 == 0 and input % 5 == 0:
    print("FizzBuzz!")
  elif input % 5 == 0:
    print("Buzz!")
  elif input % 3 == 0:
    print("Fizz!")
  else:
    print(input)


fizz_buzz(16)


def fizz_buzz(input):
  if (input % 3 == 0) and (input % 5 == 0):
    return "FizzBuzz"
  if input % 5 == 0:
    return "Buzz"
  if input % 3 == 0:
    return "Fizz"
  return input


print(fizz_buzz(15))
