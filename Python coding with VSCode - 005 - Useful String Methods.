course = "   Python programming   "
print(course.upper())
print(course.lower())
print(course.title())
print(course.strip())
print(course.lstrip())
print(course.rstrip())
print(course.find("Pro"))
print(course.find("pro"))
print(course.replace("P", "T"))
print("Programming" in course)
print("programming" in course)
print("programming" not in course)
print("Programming" not in course)
