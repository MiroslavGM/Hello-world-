price = float(input("Type price: "))
print("More than 10 and less than 30: ")
print(price > 10 and price < 30)
print("More than 10 and less than 20: ")
print(price > 10 and price < 20)
print("More than 10 or less than 30: ")
print(price > 10 or price < 30)
print("Opposite boolean if more than 10: ")
print(not price > 10)
