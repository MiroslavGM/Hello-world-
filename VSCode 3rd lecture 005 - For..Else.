successful = False
for number in range(3):
  print("Attempt!")
  if successful:
      print("Successful!")
      break
else:
  print("We tried 3 times and we failed!")

successful = True
for number in range(3):
  print("Attempt!")
  if successful:
      print("Successful!")
      break
else:
  print("We tried 3 times and we failed!")
